  "scope": "/bytes/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "any",
  "background_color": "#22272e",
  "theme_color": "#22272e",
  "id": "/bytes/",
