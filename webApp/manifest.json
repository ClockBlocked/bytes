  "file_handlers": [
    {
      "action": "https://clockblocked.github.io/bytes/index.html?action=open",
      "accept": {
        "text/javascript": [".js", ".mjs", ".cjs"],
        "text/css": [".css"],
        "text/html": [".html", ".htm"],
        "application/json": [".json"],
        "text/markdown": [".md", ".markdown"],
        "text/plain": [".txt"],
        "text/x-python": [".py"],
        "application/x-php": [".php"],
        "text/x-yaml": [".yaml", ".yml"],
        "text/xml": [".xml"],
        "text/x-sql": [".sql"],
        "text/x-typescript": [".ts", ".tsx"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+gitdev",
      "url": "https://clockblocked.github.io/bytes/index.html?repo=%s"
