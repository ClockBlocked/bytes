  "shortcuts": [
    {
      "name": "Browse Repositories",
      "short_name": "Repos",
      "description": "View and select from your repositories",
      "url": "/bytes/index.html#repo",
      "icons": [
        {
          "src": "icons/shortcut-repo.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "New File",
      "short_name": "New",
      "description": "Create a new file in your repository",
      "url": "/bytes/index.html?action=new-file",
      "icons": [
        {
          "src": "icons/shortcut-new-file.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "File Explorer",
      "short_name": "Files",
      "description": "Browse files in your current repository",
      "url": "/bytes/index.html#explorer",
      "icons": [
        {
          "src": "icons/shortcut-explorer.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Icon Library",
      "short_name": "Icons",
      "description": "Browse and copy SVG icons",
      "url": "/bytes/awesome/index.html",
      "icons": [
        {
          "src": "icons/shortcut-icons.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/bytes/index.html",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/bytes/index.html?action=open",
      "accept": {
        "text/javascript": [".js", ".mjs", ".cjs"],
        "text/css": [".css"],
        "text/html": [".html", ".htm"],
        "application/json": [".json"],
        "text/markdown": [".md", ".markdown"],
        "text/plain": [".txt"],
        "text/x-python": [".py"],
        "application/x-php": [".php"],
        "text/x-yaml": [".yaml", ".yml"],
        "text/xml": [".xml"],
        "text/x-sql": [".sql"],
        "text/x-typescript": [".ts", ".tsx"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+gitdev",
      "url": "/bytes/index.html?repo=%s"
    }
  ],
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "handle_links": "preferred",
  "edge_side_panel": {
    "preferred_width": 480
  },
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://clockblocked.github.io/bytes/webApp/manifest.json"
    }
  ]
}